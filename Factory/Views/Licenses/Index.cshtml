@{
  Layout = "_Layout";
}

@using Factory.Models;

<h1>Licenses</h1>

@if (@Model.Count == 0)
{
  <h3>No licenses have been added yet!</h3>
}

@foreach (License license in Model)
{
  <li>@Html.ActionLink($"{license.Title}", "Details", new { id = license.LicenseId })</li>
}

<p>@Html.ActionLink("Add new license", "Create")</p>

<p>@Html.ActionLink("Home", "Index", "Home")</p>